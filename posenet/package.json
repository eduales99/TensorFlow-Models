{
  "name": "@tensorflow-models/posenet",
  "version": "0.0.1",
  "description": "Pretrained PoseNet model in tensorflow.js",
  "main": "dist/index.js",
  "unpkg": "dist/bundle.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/tensorflow/tfjs-models.git"
  },
  "peerDependencies": {
    "@tensorflow/tfjs-core": "0.8.1"
  },
  "devDependencies": {
    "@types/jasmine": "~2.5.53",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-runtime": "~6.23.0",
    "browserify": "~14.5.0",
    "browserify-shim": "~3.8.14",
    "jasmine-core": "^3.1.0",
    "karma": "~1.7.0",
    "karma-browserstack-launcher": "~1.3.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-firefox-launcher": "~1.0.1",
    "karma-jasmine": "~1.1.0",
    "karma-typescript": "~3.0.8",
    "@tensorflow/tfjs-core": "0.8.1",
    "tslint": "~5.8.0",
    "tsify": "~3.0.1",
    "typescript": "2.7.2",
    "uglifyjs": "~2.4.11",
    "watchify": "~3.11.0"
  },
  "scripts": {
    "build": "tsc --sourceMap false && browserify -g browserify-shim --standalone posenet src/index.ts -p [tsify] -o dist/bundle.js",
    "watch": "watchify --debug -v --standalone posenet src/index.ts -p [tsify] -o dist/index.js",
    "test": "karma start",
    "publish-npm": "tsc --sourceMap false && yarn build && npm publish",
    "dev": "npm run watch && cs demos && npm run watch",
    "lint": "tslint -p . -t verbose"
  },
  "browserify-shim": {
    "tf": "global:dl"
  },
  "browserify": {
    "global-transform": [
      "browserify-shim"
    ]
  },
  "license": "Apache-2.0"
}
