{
  "name": "tfjs-posenet",
  "version": "0.1.3",
  "description": "Pretrained PoseNet model in tensorflow.js",
  "main": "dist/posenet/index.js",
  "unpkg": "dist/bundle.js",
  "types": "dist/posenet/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/tensorflow/tfjs-models.git"
  },
  "peerDependencies": {
    "@tensorflow/tfjs-core": "0.6.0"
  },
  "devDependencies": {
    "@tensorflow/tfjs-core": "0.6.0",
    "@types/dat.gui": "^0.6.0",
    "@types/jasmine": "~2.5.53",
    "browserify": "~14.5.0",
    "browserify-shim": "~3.8.14",
    "clang-format": "~1.2.2",
    "dat.gui": "^0.7.1",
    "jasmine-core": "~2.6.4",
    "karma": "~1.7.0",
    "karma-browserstack-launcher": "~1.3.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-typescript": "~3.0.8",
    "mkdirp": "~0.5.1",
    "shared-tslint-rules": "^3.0.0",
    "tsify": "~3.0.3",
    "tslint": "~5.8.0",
    "typescript": "2.7.2",
    "uglifyjs": "~2.4.11",
    "watchify": "~3.11.0"
  },
  "scripts": {
    "prep": "yarn && mkdirp dist",
    "build": "tsc --sourceMap false && browserify -g browserify-shim --standalone posenet src/index.ts -p [tsify] -o dist/bundle.js",
    "test": "karma start",
    "deploy-demo": "cd demos/ && gcloud app deploy -v cl-deeplearnjs-posenet --project cl-move-mirror --no-promote",
    "build-demo": "browserify --debug --standalone demos demos/index.ts -p [tsify] -o demos/bundle.js",
    "watch-demo": "watchify --debug -v --standalone demos demos/index.ts -p [tsify] -o demos/bundle.js",
    "simple-server": "cd demos && python -m SimpleHTTPServer",
    "dev": "npm run watch-demo & npm run simple-server",
    "lint": "tslint -p . -t verbose",
    "publish-npm": "tsc --sourceMap false && yarn build && npm publish"
  },
  "browserify-shim": {
    "deeplearn": "global:dl"
  },
  "browserify": {
    "global-transform": [
      "browserify-shim"
    ]
  },
  "license": "Apache-2.0"
}
